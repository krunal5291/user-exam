<% include layout %>
<div>
  <h1>Login</h1>
  <form action="/login" method="POST">
    <div>
      <label for="email">Email</label>
      <input type="email" name="email" required>
    </div>
    <div>
      <label for="password">Password</label>
      <input type="password" name="password" required>
    </div>
    <button type="submit">Login</button>
  </form>
</div>
